
{
  "cache_dir": "./src/test/resources/warc/cache/optimizerTests/feedback",

  "gold_standard_configuration": {
    "path": "./src/test/resources/warc/answer.csv",
    "hasHeader": "true"
  },

  "source_artifact_provider": {
    "name": "text",
    "args": {
      "artifact_type": "requirement",
      "path": "./src/test/resources/warc/high"
    }
  },
  "target_artifact_provider": {
    "name": "text",
    "args": {
      "artifact_type": "requirement",
      "path": "./src/test/resources/warc/low"
    }
  },
  "source_preprocessor" : {
    "name" : "artifact",
    "args" : {}
  },
  "target_preprocessor" : {
    "name" : "artifact",
    "args" : {}
  },
  "embedding_creator" : {
    "name" : "openai",
    "args" : {
      "model": "text-embedding-3-large"
    }
  },
  "source_store" : {
    "name" : "custom",
    "args" : { }
  },
  "target_store" : {
    "name" : "cosine_similarity",
    "args" : {
      "max_results" : "4"
    }
  },
  "prompt_optimizer": {
    "name" : "feedback_openai",
    "args" : {
      "prompt": "Question: Here are two parts of software development artifacts.\n\n            {source_type}: '''{source_content}'''\n\n            {target_type}: '''{target_content}'''\n            Are they related?\n\n            Answer with 'yes' or 'no'.",
      "model": "gpt-4o-mini-2024-07-18",
      "maximum_iterations": 5,
      "feedback_size": 3
    }
  },
  "classifier" : {
    "name" : "mock",
    "args" : {}
  },
  "result_aggregator" : {
    "name" : "any_connection",
    "args" : {}
  },
  "tracelinkid_postprocessor" : {
    "name" : "identity",
    "args" : {}
  },
  "metric" : {
    "name" : "f1",
    "args" : {}
  },
  "evaluator" : {
    "name" : "mock",
    "args" : {}
  }
}
